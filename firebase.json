{
  "hosting": {
    "site": "private-sauna-availability",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "fukuoka-sauna-checker",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
